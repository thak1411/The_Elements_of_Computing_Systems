/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    Nand(a = a, b = b, out = v);
    Or(a = a, b = b, out = w);
    And(a = v, b = w, out = out);
}